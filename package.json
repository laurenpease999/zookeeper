{
  "name": "zookeeper",
  "version": "1.0.0",
  "description": "critter adoption",
  "main": "dist/main/App.js",
  "source": "src/App.js",
  "targets": {
    "main": {
      "node": [
        "^12.13.1"
      ]
    }
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@frontendmasters/pet": "^1.0.3",
